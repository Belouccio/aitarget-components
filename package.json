{
  "name": "aitarget-components",
  "version": "0.0.0-semantically-released",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "commit": "git add --all . && git-cz",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "test:single": "karma start karma.conf.js --single-run",
    "gh-pages": "ng github-pages:deploy",
    "pree2e": "ng build && (lite-server -c config/bs-config.js &) && webdriver-manager update",
    "e2e": "protractor --params.login.email=$EMAIL --params.login.pass=$PASS",
    "poste2e": "killall lite-server",
    "dev": "npm run build -- --env dev",
    "build": "webpack --config ./config/webpack.config.babel.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run lint && npm run test:single && npm run e2e"
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "browser": "lib/components/main.js",
  "main": "lib/components/main.js",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "@angular/upgrade": "^2.0.0",
    "@angular2-material/core": "2.0.0-alpha.8-2",
    "@angular2-material/slider": "2.0.0-alpha.8-2",
    "@types/hammerjs": "^2.0.30",
    "@types/jasmine": "^2.2.30",
    "@types/leaflet": "1.0.35",
    "@types/lodash": "^4.14.34",
    "angular-cli": "1.0.0-beta.17",
    "codelyzer": "^1.0.0-beta.2",
    "commitizen": "^2.8.6",
    "core-js": "^2.4.1",
    "cz-conventional-changelog": "^1.2.0",
    "ghooks": "^1.3.2",
    "hammerjs": "2.0.8",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.7.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "leaflet": "1.0.1",
    "lite-server": "^2.2.2",
    "ng2-translate": "^3.1.2",
    "protractor": "4.0.9",
    "rxjs": "5.0.0-beta.12",
    "semantic-release": "^4.3.5",
    "ts-node": "1.5.2",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "webpack": "2.1.0-beta.25",
    "webpack-shell-plugin": "^0.4.3",
    "zone.js": "^0.6.23"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aitarget/aitarget-components.git"
  }
}
