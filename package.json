{
  "name": "aitarget-components",
  "version": "0.0.9-1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "gh-pages": "ng github-pages:deploy",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor --params.login.email=$EMAIL --params.login.pass=$PASS",
    "precie2e": "(lite-server -c config/bs-config.js &) && webdriver-manager update",
    "cie2e": "protractor --params.login.email=$EMAIL --params.login.pass=$PASS",
    "postcie2e": "killall lite-server",
    "dev": "npm run build -- --env dev",
    "build": "webpack --config ./config/webpack.config.babel.js",
    "deploy": "sh ./scripts/npm/deploy.sh"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "@angular/upgrade": "^2.0.0",
    "@angular2-material/core": "2.0.0-alpha.8-2",
    "@angular2-material/slider": "2.0.0-alpha.8-2",
    "core-js": "^2.4.1",
    "hammerjs": "2.0.8",
    "leaflet": "1.0.1",
    "ng2-translate": "^2.5.0",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "add-stream": "1.0.0",
    "@types/jasmine": "^2.2.30",
    "@types/lodash": "^4.14.34",
    "@types/hammerjs": "^2.0.30",
    "@types/leaflet": "1.0.34",
    "angular-cli": "1.0.0-beta.14",
    "codelyzer": "~0.0.26",
    "conventional-changelog": "1.1.0",
    "conventional-commits-parser": "1.2.3",
    "conventional-changelog-writer": "1.4.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "get-pkg-repo": "1.3.0",
    "git-raw-commits": "1.1.2",
    "git-semver-tags": "1.1.2",
    "lite-server": "2.2.2",
    "protractor": "4.0.5",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2",
    "webpack-shell-plugin": "^0.4.3"
  }
