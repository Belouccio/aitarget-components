{
  "name": "aitarget-components",
  "version": "0.0.0-semantically-released",
  "description": "Application and aitarget-components library",
  "author": "Yuri Malakhov <yuri.malakhov87@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/aitarget/aitarget-components.git"
  },
  "engines": {
    "node": "6.8.1",
    "npm": "3.10.9"
  },
  "scripts": {
    "postinstall": "(npm run clean:overlaycss && npm run build:dll && npm run build:prod) || exit 0",
    "rimraf": "rimraf",
    "webdev": "webpack-dev-server",
    "webdev:hmr": "npm run webdev -- --inline --hot",
    "webpack": "webpack",
    "ngc": "./node_modules/.bin/ngc -p tsconfig.aot.json && npm run rimraf -- output",
    "webdriver-manager": "webdriver-manager",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "protractor": "protractor",
    "pree2e:only": "npm run webdriver:update -- --standalone",
    "e2e": "npm run sass && npm-run-all -p -r e2e:server e2e:only",
    "e2e:testall": "npm run e2e:jit && npm run e2e:aot",
    "e2e:aot": "npm run compile && npm run e2e",
    "e2e:jit": "npm run build && npm run e2e",
    "e2e:only": "npm run protractor --params.login.email=$EMAIL --params.login.pass=$PASS",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e:server": "node prodserver",
    "pretest": "npm run lint && npm run sass",
    "pretest:once": "npm run lint && npm run sass",
    "pretest:once:ci": "npm run lint",
    "test:once": "karma start",
    "test:once:ci": "karma start",
    "test": "karma start",
    "ci": "npm run e2e && npm run test:once:ci",
    "ci:testall": "npm run e2e:testall && npm run test:once:ci",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/app/**/*.ts\" \"src/lib/**/*.ts\"",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc typings coverage dist .awcache dll && npm run clean:compile",
    "clean:dist": "npm run rimraf -- dist .awcache",
    "clean:dll": "npm run rimraf -- dll",
    "clean:compile": "npm run rimraf -- \"src/compiled\" ",
    "clean:overlaycss": "npm run rimraf -- \"node_modules/@angular/material/core/overlay/overlay.css\" ",
    "compile": "npm run compile:aot",
    "compile:aot": "npm run sass && npm run clean:compile && npm run ngc && npm run build:aot",
    "compile:dev": "npm run sass && npm run clean:compile && npm run ngc && npm run build:aot:dev",
    "compile:only": "npm run clean:compile && npm run sass && npm run ngc",
    "compile:watch": "watch-run -i npm run compile:only -p 'src/app/**/*.ts, src/app/**/*.scss' npm run compile:only",
    "prodserver": "node prodserver",
    "sass": "node-sass src -o src --include-path node_modules --output-style compressed -q",
    "sass:watch": "node-sass -w src -o src --include-path node_modules --output-style compressed -q",
    "start": "npm run prodserver",
    "start:hmr": "npm run server:hmr",
    "serve80": "sudo PORT=80 node prodserver",
    "server:dev": "npm-run-all -p -r webdev sass:watch",
    "server:hmr": "npm-run-all -p -r webdev:hmr sass:watch",
    "server:prod": "npm-run-all -p -r watch:prod prodserver",
    "server:prod80": "npm-run-all -p -r serve80 watch:prod",
    "debug:build": "node-nightly --inspect --debug-brk node_modules/webpack/bin/webpack.js",
    "build": "npm run build:prod",
    "build:aot": "npm run build:aot:prod",
    "build:aot:prod": "npm run clean:dist && npm run sass && webpack",
    "build:aot:dev": "npm run clean:dist && npm run sass && webpack",
    "build:dev": "npm run clean:dist && npm run sass && webpack",
    "build:dll": "npm run clean:dll && npm run sass && webpack",
    "build:prod": "npm run clean:dist && npm run sass && webpack",
    "watch": "npm run watch:dev",
    "watch:dev": "npm-run-all -p -r \"build:dev -- --watch\" sass:watch",
    "watch:prod": "npm-run-all -p -r \"build:prod -- --watch\" sass:watch",
    "commit": "git add --all . && git-cz",
    "validate": "npm run lint && npm run ci:testall",
    "gh-pages": "ng github-pages:deploy",
    "dev": "npm run build:lib -- --env dev",
    "build:lib": "webpack --config ./config/webpack.config.ts",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "opt": {
      "in": [],
      "out": [
        "pre-commit"
      ]
    },
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "browser": "lib/components/main.js",
  "main": "lib/components/main.js",
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/compiler-cli": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/material": "2.0.0-alpha.9-3",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "@angular/tsc-wrapped": "0.3.0",
    "@angular/upgrade": "^2.0.0",
    "@angularclass/hmr": "1.2.1",
    "@angularclass/hmr-loader": "3.0.2",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/router-store": "1.2.5",
    "@ngrx/store": "2.2.1",
    "@ngrx/store-devtools": "^3.2.1",
    "@ngrx/store-log-monitor": "3.0.2",
    "@types/core-js": "~0.9.33",
    "@types/express": "~4.0.33",
    "@types/hammerjs": "~2.0.33",
    "@types/ip": "~0.0.29",
    "@types/jasmine": "~2.5.35",
    "@types/leaflet": "1.0.35",
    "@types/node": "~6.0.45",
    "@types/protractor": "~1.5.20",
    "@types/selenium-webdriver": "~2.44.29",
    "angular-cli": "1.0.0-beta.17",
    "angular2-router-loader": "0.3.2",
    "angular2-template-loader": "~0.5.0",
    "awesome-typescript-loader": "~2.2.4",
    "codelyzer": "~1.0.0-beta.1",
    "commitizen": "^2.8.6",
    "compression": "1.6.2",
    "compression-webpack-plugin": "~0.3.1",
    "copy-webpack-plugin": "~3.0.1",
    "core-js": "2.4.1",
    "css-loader": "~0.25.0",
    "cz-conventional-changelog": "^1.2.0",
    "es6-promise": "~3.3.0",
    "es6-promise-loader": "~1.0.2",
    "express": "4.14.0",
    "ghooks": "^1.3.2",
    "hammerjs": "~2.0.8",
    "html-webpack-plugin": "~2.22.0",
    "ip": "~1.1.3",
    "istanbul-instrumenter-loader": "~0.2.0",
    "json-loader": "~0.5.4",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-jasmine": "~1.0.2",
    "karma-mocha-reporter": "~2.2.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-remap-coverage": "~0.1.1",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "leaflet": "1.0.1",
    "ng2-translate": "^3.1.2",
    "ngrx-store-freeze": "0.1.4",
    "ngrx-store-logger": "0.1.7",
    "node-sass": "~3.10.1",
    "npm-run-all": "~3.1.0",
    "opt-cli": "^1.5.1",
    "phantomjs-polyfill": "~0.0.2",
    "phantomjs-prebuilt": "~2.1.13",
    "protractor": "~4.0.8",
    "raw-loader": "~0.5.1",
    "rimraf": "^2.5.4",
    "rxjs": "5.0.0-beta.12",
    "semantic-release": "^4.3.5",
    "source-map-loader": "~0.1.5",
    "string-replace-loader": "~1.0.5",
    "to-string-loader": "~1.1.5",
    "ts-helpers": "~1.1.1",
    "ts-node": "~1.4.0",
    "tslint": "~3.15.1",
    "tslint-loader": "~2.1.5",
    "typescript": "~2.0.3",
    "watch-run": "~1.2.4",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-middleware": "~1.8.1",
    "webpack-dev-server": "2.1.0-beta.4",
    "webpack-shell-plugin": "^0.4.3",
    "zone.js": "0.6.25"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aitarget/aitarget-components/issues"
  },
  "homepage": "https://github.com/aitarget/aitarget-components#readme"
}
